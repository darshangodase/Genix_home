{
    "version": 2,
    "builds": [
      {
        "src": "wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "wsgi.py"
      }
    ],
    "env": {
        "FLASK_ENV": "production",
        "PYTHONPATH": "."
    }
  }
  